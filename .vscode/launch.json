{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Remote Attach",
            "type": "python",
            "request": "attach",
            "connect": {
                "host": "localhost",
                "port": 5678
            },
            "pathMappings": [
                {
                    "localRoot": "${workspaceFolder}",
                    "remoteRoot": "."
                }
            ],
            "justMyCode": true
        },
        {
            "type": "firefox",
            "request": "attach",
            "name": "Attach to Firefox",
        },
        {
            "name": "Direct search",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/benchmarks/direct.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "ENV_FOR_DYNACONF": "development"
            },
        },
        {
            "name": "Local search",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/benchmarks/local_search.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "ENV_FOR_DYNACONF": "development"
            },
        },
        {
            "name": "Simulated Annealing",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/benchmarks/simulated_annealing.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "ENV_FOR_DYNACONF": "development"
            },
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Webpack serve",
            "program": "node_modules/.bin/webpack-cli",
            "args": [
                "serve",
                "--config",
                "webpack.config.js"
            ],
            "cwd": "${workspaceFolder}/app",
            "autoAttachChildProcesses": true,
            "stopOnEntry": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Webpack build",
            "program": "node_modules/.bin/webpack-cli",
            "args": [
                "--config",
                "webpack.config.js"
            ],
            "cwd": "${workspaceFolder}/app",
            "autoAttachChildProcesses": true,
            "stopOnEntry": true
        }
    ]
}